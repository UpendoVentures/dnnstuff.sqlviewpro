<?xml version="1.0"?>
<settings>
  <sections>
    <section name="Chart">
      <propertygroup layout="LeftToRight" width="75%">
        <property name="ChartWidth" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" required="true" />
        <property name="ChartHeight" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" required="true" />
      </propertygroup>
    </section>
    <section name="Background">
      <property name="BgColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="ColorSet" type="Choice" default="">
        <choices>
          <choice value="FF0000,BF3030,A60000,FF4040,FF7373" caption="Red" />
          <choice value="1240AB,2A4480,06266F,4671D5,6C8CD5" caption="Blue" />
          <choice value="00CC00,269926,008500,39E639,67E667" caption="Green" />
          <choice value="FFFF00,BFBF30,A6A600,FFFF40,FFFF73" caption="Yellow" />
          <choice value="FF7400,BF7130,A64B00,FF9640,FFB273" caption="Orange" />
          <choice value="3914AF,412C84,200772,6A48D7,876ED7" caption="Purple" />
          <choice value="990000,CC9933,339933,339999,333399,CC6623" caption="Bright" />
          <choice value="FF85A8,FFD685,FFF385,85FFD0,85C2FF,FF85F3" caption="Pastel" />
          <choice value="Custom" caption="Custom (specify below)" />
        </choices>
      </property>
      <property name="CustomColorSet" type="String" default="" columns="90" />
      <property name="BgAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="BgSWF" type="String" default="" columns="90" />
    </section>
    <section name="Canvas">
      <property name="CanvasBgColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="CanvasBgAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="CanvasBorderColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="CanvasBorderThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="CanvasBaseColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="CanvasBaseDepth" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="CanvasBgDepth" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="ShowCanvasBg" type="Boolean" default="" />
      <property name="ShowCanvasBase" type="Boolean" default="" />
    </section>
    <section name="Chart and Axis Titles">
      <property name="Caption" type="String" default="" columns="90" />
      <property name="SubCaption" type="String" default="" columns="90" />
      <property name="XAxisName" type="String" default="" columns="90" />
      <property name="YAxisName" type="String" default="" columns="90" />
    </section>
    <section name="Chart Numerical Limits">
      <property name="YAxisMinValue" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="YAxisMaxValue" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
    </section>
    <section name="Generic Properties">
      <property name="ShowNames" type="Boolean" default="" />
      <property name="ShowValues" type="Boolean" default="" />
      <property name="ShowLimits" type="Boolean" default="" />
      <property name="RotateNames" type="Boolean" default="" />
      <property name="Animation" type="Boolean" default="" />
      <property name="ShowColumnShadow" type="Boolean" default="" />
      <property name="ShowPercentageValues" type="Boolean" default="" filter="Pie2D,Pie3D,Doughnut2D" />
      <property name="ShowPercentageInLabel" type="Boolean" default="" filter="Pie2D,Pie3D,Doughnut2D" />
      <property name="ShowBarShadow" type="Boolean" default="" />
      <property name="ShowLegend" type="Boolean" default="" />
    </section>
    <section name="Font Properties">
      <property name="BaseFont" type="String" default="" columns="90" />
      <property name="BaseFontSize" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="BaseFontColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="OutCnvBaseFont" type="String" default="" columns="90" />
      <property name="OutCnvBaseFontSize" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="OutCnvBaseFontColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
    </section>
    <section name="Number Formatting Options">
      <property name="NumberPrefix" type="String" default="" columns="15" />
      <property name="NumberSuffix" type="String" default="" columns="15" />
      <property name="FormatNumber" type="Boolean" default="" />
      <property name="FormatNumberScale" type="String" default="" columns="15" />
      <property name="DecimalSeparator" type="String" default="" columns="15" />
      <property name="ThousandSeparator" type="String" default="" columns="15" />
      <property name="DecimalPrecision" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="DivLineDecimalPrecision" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="LimitsDecimalPrecision" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
    </section>
    <section name="Zero Plane">
      <property name="ZeroPlaneThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="ZeroPlaneColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="ZeroPlaneAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="ZeroPlaneShowBorder" type="Boolean" default="" />
      <property name="ZeroPlaneBorderColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
    </section>
    <section name="Divisional Lines">
      <property name="NumDivLines" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="DivLineColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="DivLineThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="DivLineAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="ShowDivLineValue" type="Boolean" default="" />
    </section>
    <section name="Divisional Lines (Horizontal)">
      <property name="ShowAlternateHGridColor" type="Boolean" default="" />
      <property name="AlternateHGridColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="AlternateHGridAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="NumHDivLines" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="HDivLineColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="HDivLineThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="HDivLineAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
    </section>
    <section name="Divisional Lines (Vertical)">
      <property name="ShowAlternateVGridColor" type="Boolean" default="" />
      <property name="AlternateVGridColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="AlternateVGridAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="NumVDivLines" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="VDivLineColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="VDivLineThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="VDivLineAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
    </section>
    <section name="Hover Caption Properties">
      <property name="ShowHoverCap" type="Boolean" default="" />
      <property name="HoverCapBgColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="HoverCapBorderColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="HoverCapSepChar" type="String" default="" columns="15" />
    </section>
    <section name="Chart Margins">
      <property name="ChartLeftMargin" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="ChartRightMargin" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="ChartTopMargin" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="ChartBottomMargin" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
    </section>
    <section name="Pie Properties">
      <property name="PieRadius" type="String" default="" filter="Pie2D,Pie3D,Doughnut2D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="PieBorderThickness" type="String" default="" filter="Pie2D,Pie3D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="PieBorderAlpha" type="String" default="" filter="Pie2D,Pie3D" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="PieFillAlpha" type="String" default="" filter="Pie2D,Pie3D" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="PieSliceDepth" type="String" default="" filter="Pie2D,Pie3D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="PieYScale" type="String" default="" filter="Pie2D,Pie3D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
    </section>
    <section name="Shadow Properties">
      <property name="ShowShadow" type="Boolean" default="" filter="Line2D,MSLine2D,Pie2D,Doughnut2D" />
      <property name="ShadowColor" type="String" default="" filter="Line2D,MSLine2D,Pie2D,Doughnut2D" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="ShadowThickness" type="String" default="" filter="Line2D,MSLine2D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="ShadowAlpha" type="String" default="" filter="Line2D,MSLine2D,Pie2D" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="ShadowXShift" type="String" default="" filter="Line2D,MSLine2D,Pie2D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="ShadowYShift" type="String" default="" filter="Line2D,MSLine2D,Pie2D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
    </section>
    <section name="Name/Value display distance control">
      <property name="SlicingDistance" type="String" default="" filter="Pie2D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="NameTBDistance" type="String" default="" filter="Pie2D,Doughnut2D" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
    </section>
    <section name="Line Properties" filter="Line2D,MSLine2D">
      <property name="LineColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="LineThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="LineAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
    </section>
    <section name="Anchor properties" filter="Line2D,MSLine2D">
      <property name="ShowAnchors" type="Boolean" default="" />
      <property name="AnchorSides" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="AnchorRadius" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="AnchorBorderColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="AnchorBorderThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="AnchorBgColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="AnchorBgAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
      <property name="AnchorAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
    </section>
    <section name="Area Properties" filter="Area2D,MSArea2D,StackedArea2D">
      <property name="ShowAreaBorder" type="Boolean" default="" />
      <property name="AreaBorderThickness" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers allowed" />
      <property name="AreaBorderColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="AreaBgColor" type="String" default="" columns="15"  validationexpression="Colour" validationmessage="Must be a valid hex colour code (RRGGBB) (ie. 3AD055)" />
      <property name="AreaAlpha" type="String" default="" columns="10" validationexpression="Integer" validationmessage="Only numbers (0-100) allowed" />
    </section>
  </sections>
</settings>